{
  "name": "my-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "npm run test:unit",
    "test:unit": "vitest run --config vitest.config.js --coverage --dir tests/unit && node scripts/print-coverage.mjs",
    "test:integration": "vitest run --config vitest.integration.config.js --coverage --dir tests/integration && node scripts/print-coverage.mjs",
    "test:functional": "start-server-and-test dev http://localhost:5173 \"cypress run\"",
    "test:perf": "npm run build && start-server-and-test preview http://localhost:4173 \"node scripts/run-lighthouse.mjs\"",
    "test:watch": "vitest watch --config vitest.config.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.81.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/vite": "^4.1.14",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^2.1.9",
    "autoprefixer": "^10.4.21",
    "cypress": "^13.15.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^25.0.1",
    "lighthouse": "^12.3.0",
    "msw": "^2.7.0",
    "postcss": "^8.5.6",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^4.1.14",
    "vite": "npm:rolldown-vite@^7.1.14",
    "vitest": "^2.1.8"
  }
}
